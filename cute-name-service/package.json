{
  "name": "nodejs-cache-cute-name",
  "version": "1.0.0",
  "main": "false",
  "author": "Red Hat, Inc.",
  "license": "Apache-2.0",
  "scripts": {
    "test": "tape test/*.js | tap-spec",
    "lint": "xo",
    "prepare": "echo 'To confirm CVE compliance, run \"npm run security-check\"' ",
    "security-check": "nsp check",
    "coverage": "istanbul cover tape test/*.js",
    "dependencyCheck": "szero . --ci",
    "prepublish": "license-reporter report",
    "openshift": "nodeshift --strictSSL=false --nodeVersion=8.x",
    "postinstall": "license-reporter report && license-reporter save --xml licenses.xml",
    "start": "PORT=8080 node ./bin/www"
  },
  "xo": {
    "space": 2,
    "rules": {
      "space-before-function-paren": [
        "error",
        "always"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bucharest-gold/nodejs-cache.git"
  },
  "files": [
    "package.json",
    "LICENSE",
    "app.js",
    "lib",
    "bin",
    "licenses"
  ],
  "bugs": {
    "url": "https://github.com/bucharest-gold/nodejs-cache/issues"
  },
  "homepage": "https://github.com/bucharest-gold/nodejs-cache",
  "devDependencies": {
    "nodeshift": "~1.6.0",
    "nsp": "~2.6.2",
    "szero": "^0.7.1",
    "tap-spec": "~4.1.1",
    "tape": "~4.6.2",
    "xo": "~0.20.3"
  },
  "description": "",
  "keywords": [],
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "~2.8.4",
    "express": "^4.16.0",
    "kube-probe": "~0.1.2",
    "license-reporter": "^1.1.3",
    "project-name-generator": "~2.1.4",
    "roi": "~0.18.0"
  }
}
